      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Aurlyn.esp" ],
          "signature": [ "ARMO", "BOOK", "INGR", "WEAP" ]
        }
      }
    },

    {
      "name": "Rigmor of Bruma",
      "description": "Items added by Rigmor of Bruma",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Rigmor.esp" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Rigmor of Cyrodiil",
      "description": "Items added by Rigmor of Cyrodiil",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "RigmorCyrodiil.esm" ],
          "signature": [ "ARMO", "BOOK", "INGR", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Bordering on Obsession",
      "description": "Items added by Joop van Die Borders trilogy",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Folkstead.esp", "Jehanna.esp", "Nyhus.esp" ],
          "signature": [ "ARMO", "BOOK", "INGR", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Back From The Brink",
      "description": "Items added by Carved Brink",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Haem Projects Goblands.esp" ],
          "signature": [ "ALCH", "ARMO", "BOOK", "INGR", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Hammet Got them All Yet",
      "description": "Items added by Hammet's Dungeons",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "HammetDungeons.esm" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Vominheim",
      "description": "Items added by Land of Vominheim",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Vominheim.esp" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Sewer Rat",
      "description": "Items added by Skyrim Sewers",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "SkyrimSewers.esp" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Easier Peasier",
      "description": "Items added in Easier Riders Dungeon Pack SSE",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "EasierRidersDungeonPackSSE.esp" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Payback With Interest",
      "description": "Items added in Interesting NPCs",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "3DNPC.esp" ],
          "signature": [ "ALCH", "ARMO", "BOOK", "KEYM", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Palaeontologist",
      "description": "All the fossils added by Fossil Mining",
      "policy": {
        "action": "take",
        "notify": true,
        "repeat": true
      },
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Fossilsyum.esp" ],
          "signature": [ "ALCH", "ARMO", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "A Hot Meal",
      "description": "Every Cooked Food Item",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Complete Alchemy & Cooking Overhaul.esp" ],
          "keyword": [ "VendorItemFoodCooked" ]
        }
      }
    },

    {
      "name": "Cloaking Device",
      "description": "Every Cloak of Skyrim",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Cloaks.esp", "Cloaks - Dawnguard.esp" ],
          "signature": [ "ARMO" ],
          "keyword": [ "VendorItemClothing" ]
        }
      }
    },

    {
      "name": "Keys to the City",
      "description": "Every Key in the Forgotten City",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "ForgottenCity.esp" ],
          "subFilter": [
            {
              "operator": "OR",
              "condition": {
                "signature": [ "KEYM" ],
                "keyword": [ "VendorItemKey" ]
              }
            }
          ]
        }
      }
    },

    {
      "name": "LoTD Non-Display Items",
      "description": "Fate Cards, Paintings",
      "policy": {
        "action": "take",
        "notify": true,
        "repeat": true
      },
      "rootFilter": {
        "operator": "OR",
        "condition": {
          "keyword": [ "VendorItemFateCards" ],
          "formList": [
            {
              "listPlugin": "LegacyoftheDragonborn.esm",
              "formID": "0034675F"
            }
          ]
        }
      }
    }
  ]
}
