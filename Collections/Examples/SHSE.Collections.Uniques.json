{
  "$comment": "Definitions are allowed for up to 128 rulesets for Collections",
  "$schema": "./SHSE.SchemaCollections.json",
  "groupPolicy": {
    "action": "take",
    "notify": true,
    "repeat": false
  },
  "useMCM": true,
  "collections": [
    {
      "name": "Thirsty!",
      "description": "Items added by Bad Gremlins Collectables",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "BGCollectables.esp" ],
          "signature": [ "BOOK", "MISC" ]
        }
      }
    },

    {
      "name": "SUTch a Great Find",
      "description": "Items added by Skyrim's Unique Treasures",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "SkyrimsUniqueTreasures.esp" ],
          "signature": [ "ALCH", "ARMO", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "X Marks the Spot",
      "description": "Treasure Hunter SSE and New Treasure Hunt SSE Maps",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "TreasureHunter.esp", "treasure_hunt.esl" ],
          "signature": [ "BOOK", "KEYM" ]
        }
      }
    },

    {
      "name": "Oblivious",
      "description": "Items added by Oblivion Artifacts",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "WZOblivionArtifacts.esp" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Zimmersed",
      "description": "Items added by Zim's Immersive Artifacts",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "ZIA_Complete Pack.esp" ],
          "signature": [ "ARMO", "BOOK", "INGR", "WEAP" ]
        }
      }
    },

    {
      "name": "Novocaine Optional",
      "description": "Dragon teeth from Zim's Dragon Improvements",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Zim's Dragon Improvements.esp" ],
          "signature": [ "MISC" ]
        }
      }
    },

    {
      "name": "Fetcher",
      "description": "Items added by Morrowloot Ultimate and Patches",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "MLU.esp", "MLU - Immersive Armors.esp", "MLU - Summermyst.esp" ],
          "signature": [ "ARMO", "BOOK", "KEYM", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Artifactial Intelligence",
      "description": "Items added by Artifacts of Skyrim",
      "rootFilter": {
        "operator": "AND",
        "condition": {
          "plugin": [ "Artifacts.esp" ],
          "signature": [ "ARMO", "BOOK", "MISC", "WEAP" ]
        }
      }
    },

    {
      "name": "Philosophy 101",
